
fieldset
    a.btn-primary.btn(href="/addvendor") Add New Vendor
    legend All Vendors
    table.table.table-responsive.table-striped
        thead
            tr
                th #
                th Company Nam
                th User Name
                th Registed Date
                th Status
                th Action
        tbody(ng-repeat='n in test')
            tr
                td {{ n._id }}
                td {{ n.comp_name }}
                td {{ n.username }}
                td {{n.expiry_date | date }}
                td  Active
                td
                    a(href="/edit/{{n._id}}", class="btn btn-default") Edit
                    a(href="/view", class="btn btn-default") View
