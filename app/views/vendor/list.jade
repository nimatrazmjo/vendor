
fieldset
    legend All Vendors
    table.table.table-responsive.table-striped
        thead
            tr
                th #
                th Company Nam
                th User Name
                th Registed Date
                th Status
                th Action
        tbody(ng-repeat='n in test')
            tr
                td
                td {{ n.comp_name }}
                td {{ n.username }}
                td {{ date_converter(n.expiry_date) }}
                td  Active
                td
                    a(href="/edit/", class="btn btn-default") Edit
                    a(href="/view/", class="btn btn-default") View
